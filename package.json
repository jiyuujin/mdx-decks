{
  "name": "nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "format": "prettier --write \"**/*.{js,jsx,json,ts,tsx,md,mdx,css,html,yml,yaml,scss,sass}\" --ignore-path .gitignore",
    "lint": "eslint ./ --ext js,jsx",
    "lint:fix": "eslint ./ --ext js,jsx --fix",
    "test": "jest",
    "test-cover": "jest --coverage",
    "test:w": "npm run test-cover -- --watch"
  },
  "dependencies": {
    "@mdx-js/loader": "^1.6.22",
    "@next/mdx": "^12.0.3",
    "framer-motion": "^4.1.17",
    "next": "10.2.3",
    "preact": "10.5.15",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-swipeable": "6.2.0",
    "react-syntax-highlighter": "15.4.4",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@nekohack/eslint-config-react": "^0.3.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.5",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.4.5",
    "enzyme": "^3.11.0",
    "eslint": "^7.11.0",
    "fs": "^0.0.1-security",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.0",
    "prettier": "^2.3.2"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/jest-setup.js"
    ],
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mock__/style-mock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "collectCoverageFrom": [
      "components/**/*.{js,jsx}"
    ],
    "testMatch": [
      "**/__tests__/*.(js|jsx)"
    ],
    "testPathIgnorePatterns": [
      "./.next/",
      "./node_modules/"
    ]
  }
}
